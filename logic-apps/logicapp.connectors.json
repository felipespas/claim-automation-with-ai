{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "suffix": {
            "type": "String"
        },
        "location": {
            "type": "String"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[concat('datalake', parameters('suffix'))]",
            "location": "[parameters('location')]",
            "kind": "V1",
            "properties": {
                "api": {
                    "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', resourceGroup().location, '/managedApis/', 'azureblob')]"
                },
                "authenticatedUser": {},
                "customParameterValues": {},
                "displayName": "[concat('datalake', parameters('suffix'))]",
                "parameterValueSet": {
                    "name": "managedIdentityAuth",
                    "values": {}
                }
            }
        },
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[concat('keyvault', parameters('suffix'))]",
            "location": "[parameters('location')]",
            "kind": "V1",
            "properties": {
                "api": {
                    "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', resourceGroup().location, '/managedApis/', 'keyvault')]"
                },
                "authenticatedUser": {},
                "customParameterValues": {},
                "displayName": "[concat('keyvault', parameters('suffix'))]",
                "parameterValueSet": {
                    "name": "oauthMI",
                    "values": {
                        "vaultName": {
                            "value": "keyvault0503mvp"
                        }
                    }
                }
            }
        }
    ],
    "outputs": {}
}