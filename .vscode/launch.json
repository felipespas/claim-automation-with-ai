{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Demo code",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\.old-code\\demo\\app.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}\\.old-code\\demo",
        },
        {
            "name": "Attach to Python Functions",
            "type": "python",
            "request": "attach",
            "port": 9090,
            "preLaunchTask": "func: host start"
        }
    ]
}